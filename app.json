{
  "expo": {
    "name": "Student Productivity App",
    "slug": "student-productivity-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.studentproductivity.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {},
    "updates": {
      "enabled": false,
      "checkAutomatically": "NEVER",
      "fallbackToCacheTimeout": 0
    },
    "plugins": ["expo-font", "expo-router", "expo-asset"],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "81fc8cb4-da39-403b-a1ed-6407b004e3ff"
      }
    }
  }
}
